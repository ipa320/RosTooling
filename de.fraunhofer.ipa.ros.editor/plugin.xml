            extensions="ros1"
            class="ros.presentation.Ros1Editor"
            contributorClass="ros.presentation.Ros1ActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated ros -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
